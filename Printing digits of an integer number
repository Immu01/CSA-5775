#include <stdio.h>

int main() {
    int number, digit;

    printf("Enter an integer number: ");
    scanf("%d", &number);

    printf("Digits of the number:\n");

    // Extract and print each digit
    while (number != 0) {
        digit = number % 10;
        printf("%d\n", digit);
        number /= 10;
    }

    return 0;
}
