#include <stdio.h>

int main() {
    int n, i;
    int maxNum = 0;
    int num;

    printf("Enter the value of n: ");
    scanf("%d", &n);

    printf("Enter %d numbers:\n", n);

    for (i = 0; i < n; i++) {
        scanf("%d", &num);

        if (num > maxNum) {
            maxNum = num;
        }
    }

    printf("The biggest number is: %d\n", maxNum);

    return 0;
}
